{
  "name": "feathers-plus-admin",
  "description": "Project feathers-plus-admin",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "bsa-git",
    "email": "bsa2657@yandex.ru"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^8.11.4",
    "npm": ">= 6.4.1"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "test:all": "npm run eslint && cross-env NODE_ENV=test DEBUG=test:* npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon src/ -e js,json,vue,styl",
    "build": "nuxt build",
    "generate:nuxt": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "start": "node src/",
    "start:seed": "cross-env NODE_ENV=test node src/ --seed",
    "start-mongod": "mongod -f config/db/mongod.config.yml",
    "mocha": "mocha test/ --recursive --exit --timeout 10000",
    "generate:all": "feathers-plus generate all",
    "generate:fakes": "feathers-plus generate fakes",
    "generate:authentication": "feathers-plus generate authentication",
    "generate:graphql": "feathers-plus generate graphql",
    "generate:hook": "feathers-plus generate hook",
    "generate:test": "feathers-plus generate test"
  },
  "dependencies": {
    "@feathers-plus/graphql": "^1.9.0",
    "@feathers-plus/test-utils": "^0.3.4",
    "@feathersjs/authentication": "^2.1.11",
    "@feathersjs/authentication-jwt": "^2.0.5",
    "@feathersjs/authentication-local": "^1.2.5",
    "@feathersjs/authentication-oauth2": "^1.2.4",
    "@feathersjs/configuration": "^2.0.4",
    "@feathersjs/errors": "^3.3.4",
    "@feathersjs/express": "^1.2.7",
    "@feathersjs/feathers": "^3.2.3",
    "@feathersjs/socketio": "^3.2.6",
    "ajv": "^5.5.2",
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "crypto": "^1.0.1",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "dotenv-webpack": "^1.5.7",
    "express-useragent": "^1.0.12",
    "feathers-hooks-common": "^4.17.10",
    "feathers-mongoose": "^6.2.0",
    "graphql-resolvers-ast": "^1.4.0",
    "helmet": "^3.14.0",
    "lodash.merge": "^4.6.1",
    "merge-graphql-schemas": "^1.5.7",
    "mongodb-core": "^3.1.7",
    "mongoose": "^5.3.4",
    "nuxt": "^2.2.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "serve-favicon": "^2.5.0",
    "vee-validate": "^2.1.3",
    "vue-perfect-scrollbar": "^0.1.0",
    "vuetify": "^1.3.11",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
