{
  "options": {
    "ver": "1.0.0",
    "inspectConflicts": false,
    "semicolons": true,
    "freeze": [],
    "ts": false
  },
  "app": {
    "environmentsAllowingSeedData": "test",
    "seedData": true,
    "name": "feathers-plus-app",
    "description": "Project feathers-plus-app",
    "src": "src",
    "packager": "npm@>= 3.0.0",
    "providers": [
      "rest",
      "socketio"
    ]
  },
  "services": {
    "users": {
      "name": "users",
      "nameSingular": "user",
      "subFolder": "",
      "fileName": "users",
      "adapter": "mongoose",
      "path": "/users",
      "isAuthEntity": true,
      "requiresAuth": true,
      "graphql": true
    },
    "roles": {
      "name": "roles",
      "nameSingular": "role",
      "subFolder": "",
      "fileName": "roles",
      "adapter": "mongoose",
      "path": "/roles",
      "isAuthEntity": false,
      "requiresAuth": true,
      "graphql": true
    },
    "teams": {
      "name": "teams",
      "nameSingular": "team",
      "subFolder": "",
      "fileName": "teams",
      "adapter": "mongoose",
      "path": "/teams",
      "isAuthEntity": false,
      "requiresAuth": true,
      "graphql": true
    }
  },
  "hooks": {
    "verify-email": {
      "fileName": "verify-email",
      "camelName": "verifyEmail",
      "ifMulti": "n",
      "multiServices": [],
      "singleService": "users"
    },
    "normalize-query": {
      "fileName": "normalize-query",
      "camelName": "normalizeQuery",
      "ifMulti": "y",
      "multiServices": [
        "*app"
      ],
      "singleService": ""
    },
    "accounts-profile-data": {
      "fileName": "accounts-profile-data",
      "camelName": "accountsProfileData",
      "ifMulti": "n",
      "multiServices": [],
      "singleService": "users"
    },
    "auth": {
      "fileName": "auth",
      "camelName": "auth",
      "ifMulti": "y",
      "multiServices": [
        "*app"
      ],
      "singleService": ""
    },
    "my-log": {
      "fileName": "my-log",
      "camelName": "myLog",
      "ifMulti": "y",
      "multiServices": [
        "*app"
      ],
      "singleService": ""
    }
  },
  "authentication": {
    "strategies": [
      "local",
      "google",
      "github"
    ],
    "entity": "users"
  },
  "connections": {
    "mongoose": {
      "database": "mongodb",
      "adapter": "mongoose",
      "connectionString": "mongodb://localhost:27017/feathers_plus_app"
    }
  },
  "graphql": {
    "path": "/graphql",
    "strategy": "services",
    "sqlInterface": null,
    "requiresAuth": true,
    "name": "graphql",
    "doNotConfigure": false
  }
}
