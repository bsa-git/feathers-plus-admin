{
  "host": "HOST",
  "port": "PORT",
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "tests": {
    "environmentsAllowingSeedData": [
      "test"
    ],
    "local": {
      "password": "password"
    },
    "client": {
      "port": "PORT",
      "ioOptions": {
        "transports": [
          "websocket"
        ],
        "forceNew": true,
        "reconnection": false,
        "extraHeaders": {}
      },
      "primusOptions": {
        "transformer": "ws"
      },
      "restOptions": {
        "url": "BASE_URL"
      },
      "overriddenAuth": {
        "users": {
          "create": "noauth",
          "update": "auth",
          "patch": "auth",
          "remove": "auth"
        },
        "roles": {
          "create": "auth",
          "update": "auth",
          "patch": "auth",
          "remove": "auth"
        },
        "teams": {
          "create": "auth",
          "update": "auth",
          "patch": "auth",
          "remove": "auth"
        },
        "userTeams": {
          "create": "auth",
          "update": "auth",
          "patch": "auth",
          "remove": "auth"
        },
        "userProfiles": {
          "create": "noauth",
          "update": "auth",
          "patch": "auth",
          "remove": "auth"
        }
      }
    }
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "\\email",
      "passwordField": "password"
    },
    "google": {
      "clientID": "GOOGLE_ID",
      "clientSecret": "GOOGLE_SECRET",
      "callbackURL": "/auth/google/callback",
      "successRedirect": "HOME_PATH",
      "scope": [
        "profile openid email"
      ]
    },
    "github": {
      "clientID": "GITHUB_ID",
      "clientSecret": "GITHUB_SECRET",
      "callbackURL": "/auth/github/callback",
      "successRedirect": "HOME_PATH"
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    }
  },
  "mongodb": "mongodb://localhost:27017/feathers_plus_app"
}
